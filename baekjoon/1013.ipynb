    "# (100+1+ | 01)+\n",
    "import re\n",
    "import sys\n",
    "\n",
    "T = int(input())\n",
    "p = re.compile('(100+1+|01)+')\n",
    "for i in range(T):\n",
    "    pattern = input()\n",
    "    m = p.fullmatch(pattern)\n",
    "    if m:\n",
    "        print(\"YES\")\n",
    "    else:\n",
    "        print(\"NO\")"
