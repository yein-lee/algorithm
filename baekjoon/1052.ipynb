    "n, k = map(int, input().split())\n",
    "need = 0\n",
    "i=0\n",
    "while(True):\n",
    "    n2 = n // 2\n",
    "    n2 += n % 2\n",
    "    if(n2==k):\n",
    "        break\n",
    "    need += (n % 2) * pow(2,i)\n",
    "    print(need)\n",
    "    n = n2\n",
    "    i += 1\n",
    "print(need)"
