    "import re\n",
    "p = re.compile('[ABCDEF]{0,1}A+F+C+[ABCDEF]{0,1}')\n",
    "T = int(input())\n",
    "for i in range(T):\n",
    "    chromosome = input()\n",
    "    m = p.fullmatch(chromosome)\n",
    "    if m:\n",
    "        print(\"Infected!\")\n",
    "    else:\n",
    "        print(\"Good\")"
